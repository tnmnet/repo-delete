apiVersion: extensions/v1beta1
kind: Ingress
metadata:
  annotations:
    kubernetes.io/ingress.class: nginx
  name: vault-ui
spec:
  rules:
    - host: $DNS_PREFIX_VAULT.$INGRESS_PUBLIC_DNS
      http:
        paths:
          - path: /
            backend:
              serviceName: vault-ui
              servicePort: 8200
